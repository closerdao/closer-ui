    "eslint-config-custom": "*",
    "msw": "^1.2.1",
    "next-router-mock": "^0.9.3",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "react-simple-wysiwyg": "^2.2.2",
    "stripe": "^11.16.0"
  }
